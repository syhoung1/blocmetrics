<div class="col-md-6">
  <%= form_for @registered_app do |a| %>
  <div class="form-group">
    <%= a.label :name %>
    <%= a.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= a.label :url %>
    <%= a.text_field :url, class: "form-control" %>
  </div>
  <div class="btn-group">
    <%= a.submit "Save", class: "btn" %>
    <%= link_to "Cancel", root_path, class: "btn btn-danger" %>
  </div>
  <% end %>
</div>
