<div class="col-md-6">
  <%= form_for [current_user, @registered_app] do |a| %>
  <div class="form-group">
    <%= a.label :name %>
    <%= a.text_field :name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= a.label :URL %>
    <%= a.text_field :URL, class: "form-control" %>
  </div>
  <div class="btn-group">
    <%= a.submit "Save", class: "btn" %>
    <%= link_to "Cancel", user_registered_apps_path(current_user), class: "btn btn-danger" %>
  </div>
  <% end %>
</div>
